ul
  li.grey-text
    | #{__('t.duration')}
    span.grey-text.text-darken-2.right ~{{ cart.getDuration() }} #{__('hours')}
  li.grey-text
    | #{__('t.date')}
    span.grey-text.text-darken-2.right {{ cart.schedule.date | date: 'MMM dd' }}
  li.grey-text
      | #{__('t.time')}
      span.grey-text.text-darken-2.right {{ cart.schedule.time | date: 'HH:mm' }}    
  li.grey-text
    | #{__('t.how_often')}
    span.grey-text.text-darken-2.right {{ cart.schedule.rule.name | translate }}
      
